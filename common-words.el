(defun word-before-point ()
  "Return word before point. This function assumes that the point is right after
   the last space character. It will be used automatically when <SPACE> is hit."
  (let ((p (point))
        (q (- (point) 2)))
    (while (and (> q 1)
                (not (string-match "^[\s\t\n]+"
                                   (buffer-substring p (- q 1)))))
      (setq q (- q 1)))
    (replace-regexp-in-string "[\t\s]" "" 
                              (downcase (buffer-substring (- p 1) q)))))

(defun is-common-word (word)
  (gethash word common-words))

(defun kill-last-word-if-uncommon ()
  (interactive)
  (if (not (is-common-word (word-before-point)))
      (backward-kill-word 1)))

